<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <padding
          android:top="5dp"
          android:bottom="5dp"
          android:left="5dp"
          android:right="5dp"/>
      <gradient
          android:startColor="@color/baby_blue"
          android:endColor="@color/lilac"
          android:type="linear"
          android:angle="160"/>
      <corners
          android:radius="20dp"/>
    </shape>
  </item>
</layer-list>